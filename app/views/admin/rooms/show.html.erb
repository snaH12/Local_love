<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-8 offset-md-1'>
      <h2>ルーム詳細</h2></h2>
      <table class='table table-hover table-inverse'>
        <tr>
          <td><%= @room.name %></td>
        </tr>
      </table>
      <div class='message'>
        <% @messages.each do |message| %>
          <p><%= message.user.name %>:<%= message.message %></p>
        <% end %>
      </div>
      <diV>
        <%= link_to "戻る",admin_rooms_path %>
      </div>
    </div>
  </div>
</div>
